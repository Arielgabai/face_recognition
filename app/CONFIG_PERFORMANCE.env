# Configuration des optimisations de performance
# Copiez ces valeurs dans votre fichier .env

# === DATABASE ===
DB_POOL_SIZE=60
DB_MAX_OVERFLOW=40
DB_POOL_RECYCLE=1800
DB_POOL_TIMEOUT=60

# === PHOTO QUEUE (Traitement asynchrone) ===
PHOTO_QUEUE_MAX_SIZE=1000
PHOTO_QUEUE_WORKERS=3
PHOTO_QUEUE_BATCH_SIZE=5

# === AWS REKOGNITION ===
AWS_CONCURRENT_REQUESTS=10

